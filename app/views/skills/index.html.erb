<h1 align="center">All Skills</h1>
<div align="center">
  <%= will_paginate %>
  <% @skills.each do |skill| %>
    <ul class="listing">
      <div class="row">
        <div class="well col-md-4 col-md-offset-4">
          <li class="project-title">
            <%= link_to "#{skill.name}", skill_path(skill) %>
          </li>
          <li><small><%= pluralize(skill.users.count, "users") %></small></li>
        </div>
      </div>
    </ul>
  <% end %>
  <%= will_paginate %>
</div>